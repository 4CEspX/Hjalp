{
  "expo": {
    "plugins": [
      "react-native-nfc-manager"
    ],
    "name": "my-pp",
    "slug": "my-pp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.test.mypp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0000006"
      },
      "permissions": [
        "android.permission.NFC"
      ],
      "package": "com.test.mypp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9dddd56e-8531-48ac-8bd8-1541130305ec"
      }
    }
  }
}
